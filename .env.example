# AWS Configuration
AWS_REGION=us-east-1
AWS_ACCESS_KEY_ID=your_access_key
AWS_SECRET_ACCESS_KEY=your_secret_key

# AgentCore Configuration
AGENTCORE_GATEWAY_URL=https://your-agentcore-gateway.amazonaws.com
AGENTCORE_MEMORY_NAME=CustomerSupportMemory
AGENTCORE_ACTOR_ID=customer-support-platform
AGENTCORE_SESSION_ID=default-session

# AI Model Configuration
ANTHROPIC_API_KEY=your_anthropic_key
OPENAI_API_KEY=your_openai_key
GOOGLE_API_KEY=your_google_key

# Model IDs (Production configurable)
BEDROCK_MODEL_ID=anthropic.claude-3-haiku-20240307-v1:0
LITELLM_MODEL_ID=anthropic/claude-3-haiku-20240307

# Agent Configuration (Production configurable)
AGENT_BASE_URL=http://localhost
AGENT_HOST=127.0.0.1

# Database Configuration (Local Development)
REDIS_URL=redis://localhost:6379
POSTGRES_URL=postgresql://user:password@localhost:5432/customer_support

# DynamoDB Tables (AWS)
TICKETS_TABLE=dev-customer-support-tickets
CUSTOMERS_TABLE=dev-customer-support-customers
CHAT_SESSIONS_TABLE=dev-customer-support-chat-sessions
CONNECTIONS_TABLE=dev-customer-support-connections
FEEDBACK_TABLE=dev-customer-support-feedback

# WebSocket Configuration
WEBSOCKET_ENDPOINT=wss://your-websocket-api.execute-api.us-east-1.amazonaws.com/dev

# Application Configuration
ENVIRONMENT=development
LOG_LEVEL=INFO
DEBUG=true

# Agent Ports (Local Development)
SUPERVISOR_AGENT_PORT=9000
CLASSIFIER_AGENT_PORT=9001
SENTIMENT_AGENT_PORT=9002
KNOWLEDGE_AGENT_PORT=9003
RESOLUTION_AGENT_PORT=9004
ESCALATION_AGENT_PORT=9005
ANALYTICS_AGENT_PORT=9006
INTEGRATION_AGENT_PORT=9007
NOTIFICATION_AGENT_PORT=9008

# External Integrations
SLACK_BOT_TOKEN=xoxb-your-slack-bot-token
SLACK_SIGNING_SECRET=your-slack-signing-secret
SENDGRID_API_KEY=your-sendgrid-key

# Security
JWT_SECRET_KEY=your-jwt-secret-key
ENCRYPTION_KEY=your-encryption-key